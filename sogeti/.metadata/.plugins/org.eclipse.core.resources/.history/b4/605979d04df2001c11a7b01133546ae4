CREATE TABLE CUSTOMER (
  id INT AUTO_INCREMENT  PRIMARY KEY,
  model VARCHAR(250) NOT NULL,
  make VARCHAR(250) NULL,
  version VARCHAR(250) NOT NULL,
  no_Of_Doors int  NULL,
  co2_Emission VARCHAR(250)  NULL,
  gross_Price VARCHAR(250)  NULL,
  nett_price VARCHAR(250) NOT NULL,
  mileage VARCHAR(250) NOT NULL,
  car_id VARCHAR(250) NOT NULL,
  nettPrice VARCHAR(250) NOT NULL
);

CREATE SEQUENCE HIBERNATE_SEQUENCE START WITH 1 INCREMENT BY 1;

